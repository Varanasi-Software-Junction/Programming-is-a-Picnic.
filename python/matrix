a=[
    [1, 2, 3],
    [4, 5, 6],
    [7, 8, 9]
]
print(a)
for row in a:
    for element in row:
        print(element, end="")
    print()
b=[
    [2, 2, 2],
    [3, 3, 3],
    [4, 1, 1]
]
print(b)
for row in b:
    for element in row:
        print(element, end="")
    print()
sum=0
sum= a+b
print(sum)